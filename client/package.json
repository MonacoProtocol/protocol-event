{
  "name": "@monaco-protocol/event-client",
  "version": "0.2.1",
  "description": "Interface package for the Monaco Protocol Events program on Solana",
  "author": "Monaco Protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MonacoProtocol/protocol-event.git",
    "directory": "client"
  },
  "bugs": {
    "url": "https://github.com/MonacoProtocol/protocol-event/issues"
  },
  "homepage": "https://github.com/MonacoProtocol/protocol-event/blob/main/npm-client/README.md",
  "files": [
    "accounts/",
    "errors/",
    "instructions/",
    "queries/",
    "types/",
    "programId.ts"
  ],
  "main": "index.ts",
  "module": "index.ts",
  "scripts": {
    "build": "tsc --downlevelIteration",
    "clean": "tsc --build --clean",
    "publishClient": "npm install && npm publish --access public",
    "publishDevClient": "npm install && npm publish --tag dev --access public",
    "publishRCClient": "npm install && npm publish --tag rc --access public"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.68.0",
    "typescript": "^4.5.4"
  }
