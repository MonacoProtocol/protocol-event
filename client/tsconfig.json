{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "allowJs": true,
      "declaration": true,
      "strict": true,
      "noImplicitAny": false,
      "esModuleInterop": true,
      "preserveConstEnums": true,
      "moduleResolution": "Node",
      "allowSyntheticDefaultImports": true,
      "baseUrl": ".",
      "downlevelIteration": true,
      "skipLibCheck": true
    },
  "include": [
    "accounts/",
    "errors/",
    "instructions/",
    "queries/",
    "types/",
    "transactions/",
    "pdas/",
    "index.ts",
    "programId.ts"
  ]
  }
